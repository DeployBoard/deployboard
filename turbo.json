{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "build:schema": {
      "dependsOn": ["^build:schema"],
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["build/**", "dist/**"],
      "cache": false
    },
    "start": {
      "dependsOn": ["^start"],
      "cache": false
    },
    "test": {
      "dependsOn": ["build"],
      "outputs": [],
      "inputs": ["src/**/*.js", "src/**/*.jsx"]
    },
    "lint": {
      "outputs": []
    },
    "deploy": {
      "dependsOn": ["build", "test", "lint"],
      "outputs": [],
      "cache": false
    },
    "seed": {
      "dependsOn": ["^seed"],
      "outputs": [],
      "cache": false
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "dependsOn": ["^clean"]
    }
  }
}
