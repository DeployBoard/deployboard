{
  "name": "deploy",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "engines": {
    "node": "24.x",
    "npm": "11.x"
  },
  "scripts": {
    "dev": "PORT=3003 HOST=localhost nodemon --delay 500ms --exec babel-node ./src/index.js",
    "start": "PORT=3003 HOST=localhost node ./dist/index.js",
    "clean": "rm -rf node_modules/ dist/",
    "build": "babel src --out-dir dist",
    "prettier": "prettier --config ./.prettierrc --write \"**/*.js\"",
    "pretest": "eslint --ignore-path .gitignore .",
    "postinstall": "rm -rf dist && npm run build",
    "lint": "npm run prettier --write --check --config ./.prettierrc \"**/*.js\" && eslint --fix ./src"
  },
  "dependencies": {
    "express": "^5.2.1",
    "node-cache": "^5.1.2",
    "cors": "^2.8.5",
    "mongoose": "^9.0.2",
    "models": "*",
    "dotenv": "^17.2.3",
    "mongoose-bcrypt": "^1.11.0",
    "mongoose-timestamp": "^0.6.0",
    "loglevel": "^1.9.2"
  },
  "devDependencies": {
    "nodemon": "^3.1.11",
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.5",
    "@babel/node": "^7.28.0",
    "@babel/preset-env": "^7.28.5",
    "@babel/eslint-parser": "^7.28.5",
    "babel-loader": "^10.0.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^9.39.2",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^7.2.1",
    "fs-extra": "^11.3.3",
    "prettier": "^3.7.4"
  }
}
