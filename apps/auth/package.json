{
  "name": "auth",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "PORT=3002 HOST=localhost nodemon --delay 500ms --exec babel-node ./src/index.js",
    "start": "PORT=3002 HOST=localhost node --exec babel-node ./src/index.js",
    "clean": "rm -rf node_modules/ dist/",
    "build": "babel src --out-dir dist",
    "prettier": "prettier --config ./.prettierrc --write \"**/*.js\"",
    "pretest": "eslint --ignore-path .gitignore .",
    "postinstall": "rm -rf dist && npm run build",
    "lint": "npm run prettier --write --check --config ./.prettierrc \"**/*.js\" && eslint --fix ./src",
    "release": "release-it patch --no-npm.publish"
  },
  "dependencies": {
    "express": "^4.18.1",
    "cors": "^2.8.5",
    "mongoose": "^6.4.0",
    "models": "*",
    "body-parser": "^1.20.0",
    "dotenv": "^16.0.1",
    "mongoose-bcrypt": "^1.9.0",
    "mongoose-timestamp": "^0.6.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.18",
    "@babel/cli": "^7.18.6",
    "@babel/core": "^7.18.6",
    "@babel/node": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.5",
    "babel-preset-env": "^1.7.0",
    "eslint": "^8.18.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "fs-extra": "^10.1.0",
    "prettier": "^2.7.1"
  }
}
